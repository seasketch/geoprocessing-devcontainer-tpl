{
	"name": "Geoprocessing Codespace",
	  "dockerComposeFile": "docker-compose.yml",
	  "service": "workspace",
	  "workspaceFolder": "/workspaces",
	  "postCreateCommand": "bash /workspaces/geoprocessing-devcontainer/common/scripts/postCreateCodespace.sh",
	  "shutdownAction": "stopCompose",
	  "remoteUser": "vscode",
	  "containerUser": "vscode",
	  "customizations": {
		"codespaces": {
			"repositories": {
				"seasketch/geoprocessing": {
					"permissions": "write-all"
				},
				"seasketch/azores-nearshore-reports": {
					"permissions": "write-all"
				},
				"seasketch/brazil-reports": {
					"permissions": "write-all"
				},
				"seasketch/maldives-nearshore-reports": {
					"permissions": "write-all"
				},
				"seasketch/fsm-reports": {
					"permissions": "write-all"
				},
				"seasketch/samoa-reports": {
					"permissions": "write-all"
				},
				"seasketch/fiji-reports": {
					"permissions": "write-all"
				},
				"seasketch/belize-reports": {
					"permissions": "write-all"
				}
			}
		}
	}
  }
